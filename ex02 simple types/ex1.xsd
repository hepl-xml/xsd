<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="age">
		<xs:simpleType>
			<xs:restriction base="xs:nonNegativeInteger">
				<xs:maxExclusive value="150"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- NOT RELEVANT HERE -->
	<xs:element name="person">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="age"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
